{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@*": [
        "src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "types/next-auth.d.ts",
    "./types/next-auth.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/**/*.ts",
    "types/**/*.ts",
    "@types/next-auth.d.ts",
    "src/pages/api/auth/[...nextauth].js",
    "src/components/AuthRoute",
    "src/lib/prisma.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}